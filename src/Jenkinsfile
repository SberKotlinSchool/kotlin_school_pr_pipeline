
pipeline {
  agent any

  parameters {
          string(name: 'SOURCE_SCM')
          string(name: 'TARGET_SCM')
          string(name: 'PR_OWNER')

      }

  stages {
  stage('Build') {
      steps {

        echo 'Building...'

        echo "SOURCE_SCM = '${SOURCE_SCM}'"
        echo "TARGET_SCM = '${TARGET_SCM}'"
        echo "PR_OWNER = '${PR_OWNER}'"

      }
    }
  }
}